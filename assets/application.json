    "stores/me": [
      "GET"
    ],
    "procedures": [
      "POST"
    ],
    "products": [],
    "brands": [],
    "categories": [],
    "customers": [],
    "orders": [
      "GET"
    ],
    "carts": [
      "GET"
    ],
    "orders/fulfillments": [],
    "orders/payments_history": [],
    "products/quantity": [],
    "products/variations/quantity": [],
    "products/price": [],
    "products/variations/price": []
